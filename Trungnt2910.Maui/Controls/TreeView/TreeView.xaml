<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:Trungnt2910.Maui.Controls"
             x:Name="this"
             x:Class="Trungnt2910.Maui.Controls.TreeView">
    <ListView x:Name="ItemsView" x:FieldModifier="internal"
              HorizontalOptions="{Binding HorizontalOptions, Source={x:Reference this}}"
              VerticalOptions="{Binding VerticalOptions, Source={x:Reference this}}"
              ItemTemplate="{Binding ItemTemplate, Source={x:Reference this}}"
              Header="{Binding Header, Source={x:Reference this}}"
              HeaderTemplate="{Binding HeaderTemplate, Source={x:Reference this}}"
              Footer="{Binding Footer, Source={x:Reference this}}"
              FooterTemplate="{Binding FooterTemplate, Source={x:Reference this}}">
    </ListView>
</ContentView>
